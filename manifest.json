{
  "name": "8comic unlimit",
  "short_name": "unlimit",
  "version": "1.0.2",
  "description": "瀏覽 8comit 不換頁",
  "icons": {},
  "permissions": [
    "http://new.comicvip.com/*",
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
      {
          "matches": ["http://new.comicvip.com/*"],
          "js": ["jquery.js", "myscript.js"]
      }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
      "*.mp3",
      "*.json"
  ]
}
