{
  "name": "8comic unlimit",
  "version": "1.0.0",
  "description": "瀏覽 8comit 不換頁",
  "icons": {"16": "16.png", "48": "48.png", "64": "64.png", "128": "128.png"},
  "permissions": [
    "http://new.comicvip.com/*",
    "http://infinite.bency.org/*",
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
      {
          "matches": ["http://new.comicvip.com/*"],
          "js": ["jquery.js", "myscript.js"]
      }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
      "*.mp3",
      "*.json"
  ]
}
