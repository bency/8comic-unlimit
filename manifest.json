{
  "name": "8comic unlimit",
  "short_name": "unlimit",
  "version": "1.2",
  "description": "瀏覽 8comit 不換頁",
  "icons": {"16": "16_16_c.png", "48": "48_48_c.png", "64": "64_64_c.png", "128": "128_128_c.png"},
  "permissions": [
    "http://new.comicvip.com/*",
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
      {
          "matches": ["http://new.comicvip.com/*"],
          "js": ["jquery.js", "myscript.js"]
      }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
      "*.mp3",
      "*.json"
  ]
}
